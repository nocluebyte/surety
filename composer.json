{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-debugbar": "^3.6",
        "barryvdh/laravel-snappy": "^1.0",
        "cartalyst/sentinel": "^7.0",
        "doctrine/dbal": "^3.6",
        "egulias/email-validator": "^2.0|^3.1",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/contracts": "^10.17",
        "intervention/image": "^2.7",
        "laravel/framework": "^10.10",
        "laravel/passport": "^11.8.8",
        "laravel/tinker": "^2.8",
        "laravelcollective/html": "^6.4",
        "maatwebsite/excel": "^3.1",
        "milon/barcode": "^10.0",
        "psr/http-message": "^1.0.1",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "srlabs/centaur": "^12.0",
        "swiftmailer/swiftmailer": "^6.3",
        "venturecraft/revisionable": "^1.28",
        "yajra/laravel-datatables": "^10.0"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Nsure\\Crud\\": "nsure/crud/src/",
            "Nsure\\Flash\\": "nsure/flash/src/",
            "Nsure\\Services\\": "nsure/services",
            "Nsure\\Helper\\": "nsure/helper"
        },
        "files": [
            "app/Http/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp": true,
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
